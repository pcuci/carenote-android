<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:parentActivityName="com.health.caresnap.MainActivity"
    tools:context="com.health.caresnap.com.health.caresnap.ui.VisitViewNewActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80dp">

       <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle ="bold"
            android:text="@string/visit_view_by"
            android:id="@+id/visit_view_by"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
           android:ems="10"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/view_visit_physician_name"
            android:textStyle ="bold"
            android:layout_marginTop="10dp"
            android:ems="10"
            android:layout_below="@+id/visit_view_by"
            android:textAppearance="?android:attr/textAppearanceLarge" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="64dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle ="bold"
            android:text="@string/visit_view_at"
            android:id="@+id/textView"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:layout_marginTop="10dp"
            android:id="@+id/visit_view_clinic_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:layout_below="@+id/visit_view_at"
            android:ems="10"
            android:textAppearance="?android:attr/textAppearanceLarge" />
    </RelativeLayout>
